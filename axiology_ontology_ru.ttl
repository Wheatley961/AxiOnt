@prefix : <http://example.org/axiology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/axiology> a owl:Ontology ;
    rdfs:comment "Детальная аксиологическая онтология (OWL/Turtle) для WebProtege, основанная на Указе Президента РФ № 809 от 9 ноября 2022 г." ;
    rdfs:seeAlso <urn:decree:rf:2022:809> .

################################################################
# Классы
################################################################
:Policy a owl:Class ;
    rdfs:label "Государственная политика"@ru ;
    rdfs:comment "Высокоуровневая политика государства (например, основы государственной политики по сохранению традиционных ценностей)."@ru .

:TraditionalValue a owl:Class ;
    rdfs:label "Традиционная ценность"@ru ;
    rdfs:comment "Духовные, моральные и культурные ценности, перечисленные в Указе № 809."@ru .

:Instrument a owl:Class ;
    rdfs:label "Инструмент реализации"@ru ;
    rdfs:comment "Средства реализации государственной политики."@ru .

:Actor a owl:Class ;
    rdfs:label "Актор"@ru ;
    rdfs:comment "Субъект, участвующий в реализации политики."@ru .

:Threat a owl:Class ;
    rdfs:label "Угроза"@ru ;
    rdfs:comment "Угрозы традиционным ценностям."@ru .

:Goal a owl:Class ;
    rdfs:label "Цель"@ru ;
    rdfs:comment "Стратегическая цель государственной политики."@ru .

:Task a owl:Class ;
    rdfs:label "Задача"@ru ;
    rdfs:comment "Операционная задача для достижения цели."@ru .

:Area a owl:Class ;
    rdfs:label "Сфера реализации"@ru ;
    rdfs:comment "Отрасли и направления применения политики."@ru .

:MonitoringIndicator a owl:Class ;
    rdfs:label "Индикатор мониторинга"@ru ;
    rdfs:comment "Метрики для оценки выполнения государственной политики."@ru .

:Scenario a owl:Class ;
    rdfs:label "Сценарий"@ru ;
    rdfs:comment "Вариант развития ситуации."@ru .

:CulturalHeritageObject a owl:Class ;
    rdfs:label "Объект культурного наследия"@ru ;
    rdfs:comment "Объекты, требующие государственной защиты."@ru .

:ReligiousTradition a owl:Class ;
    rdfs:label "Религиозная традиция"@ru ;
    rdfs:comment "Традиционные религии России."@ru .


################################################################
# Подклассы инструментов
################################################################
:LegalInstrument a owl:Class ; rdfs:subClassOf :Instrument ; rdfs:label "Правовой инструмент"@ru .
:OrganizationalInstrument a owl:Class ; rdfs:subClassOf :Instrument ; rdfs:label "Организационный инструмент"@ru .
:ScientificInstrument a owl:Class ; rdfs:subClassOf :Instrument ; rdfs:label "Научно-аналитический инструмент"@ru .
:InformationalInstrument a owl:Class ; rdfs:subClassOf :Instrument ; rdfs:label "Информационный инструмент"@ru .
:FinancialInstrument a owl:Class ; rdfs:subClassOf :Instrument ; rdfs:label "Финансовый инструмент"@ru .

################################################################
# Подклассы акторов
################################################################
:StateBody a owl:Class ; rdfs:subClassOf :Actor ; rdfs:label "Государственный орган"@ru .
:FederalExecutiveBody a owl:Class ; rdfs:subClassOf :StateBody ; rdfs:label "Федеральный орган исполнительной власти"@ru .
:LawEnforcementAgency a owl:Class ; rdfs:subClassOf :StateBody ; rdfs:label "Правоохранительный орган"@ru .
:ReligiousOrganization a owl:Class ; rdfs:subClassOf :Actor, :ReligiousTradition ; rdfs:label "Религиозная организация"@ru .
:CivilSocietyOrganization a owl:Class ; rdfs:subClassOf :Actor ; rdfs:label "Общественная организация"@ru .
:MediaOrganization a owl:Class ; rdfs:subClassOf :Actor ; rdfs:label "Средство массовой информации"@ru .

################################################################
# Индивиды — традиционные ценности
################################################################
:Life a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Жизнь"@ru .
:Dignity a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Достоинство"@ru .
:HumanRightsAndFreedoms a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Права и свободы человека"@ru .
:Patriotism a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Патриотизм"@ru .
:CivicResponsibility a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Гражданская ответственность"@ru .
:ServiceToFatherland a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Служение Отечеству"@ru .
:HighMoralIdeals a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Высокие нравственные идеалы"@ru .
:StrongFamily a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Крепкая семья"@ru .
:ProductiveLabor a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Трудолюбие"@ru .
:PrioritySpiritualOverMaterial a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Приоритет духовного над материальным"@ru .
:Humanism a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Гуманизм"@ru .
:Mercy a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Милосердие"@ru .
:Justice a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Справедливость"@ru .
:Collectivism a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Коллективизм"@ru .
:MutualHelp a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Взаимопомощь"@ru .
:MutualRespect a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Взаимное уважение"@ru .
:HistoricalMemory a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Историческая память"@ru .
:UnityOfPeoples a owl:NamedIndividual ; rdf:type :TraditionalValue ; rdfs:label "Единство народов России"@ru .

################################################################
# Угрозы
################################################################
:DestructiveIdeology a owl:NamedIndividual ; rdf:type :Threat ; rdfs:label "Деструктивная идеология"@ru .
:Extremism a owl:NamedIndividual ; rdf:type :Threat ; rdfs:label "Экстремизм и терроризм"@ru .
:ForeignInfluence a owl:NamedIndividual ; rdf:type :Threat ; rdfs:label "Враждебное внешнее влияние"@ru .
:InformationPsychologicalImpact a owl:NamedIndividual ; rdf:type :Threat ; rdfs:label "Информационно‑психологическое воздействие"@ru .
:HistoricalFalsification a owl:NamedIndividual ; rdf:type :Threat ; rdfs:label "Фальсификация истории"@ru .

################################################################
# Объектные свойства
################################################################
:hasValue a owl:ObjectProperty ;
    rdfs:label "включает ценность"@ru ;
    rdfs:comment "Политика включает традиционные ценности."@ru .

:targetsThreat a owl:ObjectProperty ;
    rdfs:label "направлена против угрозы"@ru .

:implementedBy a owl:ObjectProperty ;
    rdfs:label "реализуется актором"@ru .

:usesInstrument a owl:ObjectProperty ;
    rdfs:label "использует инструмент"@ru .

:hasGoal a owl:ObjectProperty ;
    rdfs:label "имеет цель"@ru .

:hasTask a owl:ObjectProperty ;
    rdfs:label "имеет задачу"@ru .

:appliesInArea a owl:ObjectProperty ;
    rdfs:label "реализуется в сфере"@ru .

:monitoredByIndicator a owl:ObjectProperty ;
    rdfs:label "мониторится индикатором"@ru .

:hasScenario a owl:ObjectProperty ;
    rdfs:label "имеет сценарий"@ru .

:protectsHeritage a owl:ObjectProperty ;
    rdfs:label "охраняет наследие"@ru .

:supportsReligion a owl:ObjectProperty ;
    rdfs:label "поддерживает религию"@ru .


################################################################
# Датные свойства
################################################################
:hasDescription a owl:DatatypeProperty ; rdfs:label "описание"@ru .
:indicatorType a owl:DatatypeProperty ; rdfs:label "тип индикатора"@ru .
:indicatorSource a owl:DatatypeProperty ; rdfs:label "источник индикатора"@ru .
:effectiveFrom a owl:DatatypeProperty ; rdfs:label "дата вступления в силу"@ru .

################################################################
# Индивид — сама политика по Указу № 809
################################################################
:Decree809_Policy a owl:NamedIndividual ;
    rdf:type :Policy ;
    rdfs:label "Государственная политика по сохранению и укреплению традиционных духовно‑нравственных ценностей (Указ № 809)"@ru ;
    :hasDescription "Установлена Указом Президента РФ № 809 от 9 ноября 2022 года."@ru ;
    :effectiveFrom "2022-11-09"^^xsd:date ;
    :hasValue :Life , :Dignity , :HumanRightsAndFreedoms ,
              :Patriotism , :CivicResponsibility , :ServiceToFatherland ,
              :HighMoralIdeals , :StrongFamily , :ProductiveLabor ,
              :PrioritySpiritualOverMaterial , :Humanism , :Mercy ,
              :Justice , :Collectivism , :MutualHelp , :MutualRespect ,
              :HistoricalMemory , :UnityOfPeoples ;
    :targetsThreat :DestructiveIdeology , :Extremism ,
                   :ForeignInfluence , :InformationPsychologicalImpact ,
                   :HistoricalFalsification .

################################################################
# Сферы реализации
################################################################
:EducationArea a owl:NamedIndividual ; rdf:type :Area ; rdfs:label "Образование и воспитание"@ru .
:CultureArea a owl:NamedIndividual ; rdf:type :Area ; rdfs:label "Культура"@ru .
:ScienceArea a owl:NamedIndividual ; rdf:type :Area ; rdfs:label "Наука"@ru .
:InterethnicRelationsArea a owl:NamedIndividual ; rdf:type :Area ; rdfs:label "Межэтнические и межрелигиозные отношения"@ru .
:MediaArea a owl:NamedIndividual ; rdf:type :Area ; rdfs:label "Средства массовой информации"@ru .
:InternationalCooperationArea a owl:NamedIndividual ; rdf:type :Area ; rdfs:label "Международное сотрудничество"@ru .

################################################################
# Связи политики со сферами
################################################################
:Decree809_Policy :appliesInArea :EducationArea , :CultureArea , :ScienceArea ,
                                   :InterethnicRelationsArea , :MediaArea ,
                                   :InternationalCooperationArea .

################################################################
# Наследие и религии
################################################################
:CulturalHeritage a owl:NamedIndividual ; rdf:type :CulturalHeritageObject ;
    rdfs:label "Объекты культурного наследия народов России"@ru .

:Orthodoxy a owl:NamedIndividual ; rdf:type :ReligiousTradition ; rdfs:label "Православие"@ru .
:Islam a owl:NamedIndividual ; rdf:type :ReligiousTradition ; rdfs:label "Ислам"@ru .
:Buddhism a owl:NamedIndividual ; rdf:type :ReligiousTradition ; rdfs:label "Буддизм"@ru .
:Judaism a owl:NamedIndividual ; rdf:type :ReligiousTradition ; rdfs:label "Иудаизм"@ru .

:Decree809_Policy :protectsHeritage :CulturalHeritage ;
                   :supportsReligion :Orthodoxy , :Islam , :Buddhism , :Judaism .

################################################################
# Цели, задачи, индикаторы, сценарии
################################################################
:Goal_Preserve a owl:NamedIndividual ; rdf:type :Goal ;
    rdfs:label "Сохранение и укрепление традиционных ценностей"@ru .

:Task_Coordination a owl:NamedIndividual ; rdf:type :Task ; rdfs:label "Межведомственная координация"@ru .
:Task_SupportProjects a owl:NamedIndividual ; rdf:type :Task ; rdfs:label "Поддержка культурно‑образовательных проектов"@ru .
:Task_ProtectHeritage a owl:NamedIndividual ; rdf:type :Task ; rdfs:label "Охрана культурного наследия"@ru .
:Task_CounterIdeology a owl:NamedIndividual ; rdf:type :Task ; rdfs:label "Противодействие деструктивной идеологии"@ru .

:Goal_Preserve :hasTask :Task_Coordination , :Task_SupportProjects ,
                        :Task_ProtectHeritage , :Task_CounterIdeology .

:Decree809_Policy :hasGoal :Goal_Preserve .

:Indicator_Stat a owl:NamedIndividual ; rdf:type :MonitoringIndicator ;
    rdfs:label "Официальная статистика"@ru ;
    :indicatorType "статистика"@ru ;
    :indicatorSource "Росстат"@ru .

:Indicator_Survey a owl:NamedIndividual ; rdf:type :MonitoringIndicator ;
    rdfs:label "Социологические опросы"@ru ;
    :indicatorType "опрос"@ru ;
    :indicatorSource "исследовательские центры"@ru .

:Decree809_Policy :monitoredByIndicator :Indicator_Stat , :Indicator_Survey .

:Scenario_Positive a owl:NamedIndividual ; rdf:type :Scenario ;
    rdfs:label "Позитивный сценарий"@ru .

:Scenario_Negative a owl:NamedIndividual ; rdf:type :Scenario ;
    rdfs:label "Негативный сценарий"@ru .

:Decree809_Policy :hasScenario :Scenario_Positive , :Scenario_Negative .
